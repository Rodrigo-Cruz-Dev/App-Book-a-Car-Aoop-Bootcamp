        <condition/>
        <description>Validate End date, so it can't be before Start Date.</description>
        <field>end_date</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Validate End Date</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    // Exit if the form is loading or the new value is empty
    if (isLoading || newValue === '') {
        return;
    }

    var startDate = g_form.getValue('start_date'); // Get the value of the 'start_date' field

    // Only proceed if 'start_date' has a value
    if (startDate) {
        // Convert date strings to Date objects for comparison
        var endDateObj = new Date(newValue);
        var startDateObj = new Date(startDate);

        // Compare the dates
        if (endDateObj < startDateObj) {
            g_form.clearValue('end_date'); // Clear the invalid end date
            g_form.showErrorBox('end_date', getMessage('End date can not be before start date.')); // Show an error message
        } else {
            // If the date is valid, ensure any previous error message is cleared
            g_form.hideErrorBox('end_date');
        }
    }]]></script>
